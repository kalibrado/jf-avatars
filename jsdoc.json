{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "sourceType": "module",
  "source": {
    "include": [
      "./src/js", 
      "package.json",
      "README.md"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "systemName": "jf-avatars",
    "footer": "Â© 2025 Kalibrado",
    "navType": "vertical",
    "theme": "default",
    "linenums": true,
    "highlighting": "monochrome"
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true
    
  }
}
